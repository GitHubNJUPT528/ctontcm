      <if test="truckType != null">
        truck_type = #{truckType,jdbcType=VARCHAR},
      </if>
      <if test="count != null">
        count = #{count},
      </if>
      <if test="truckVolume != null">
        truck_volume = #{truckVolume,jdbcType=DOUBLE},
      </if>
      <if test="truckLoad != null">
        truck_load = #{truckLoad,jdbcType=DOUBLE},
      </if>
      <if test="fuelLitersperhundredmiles != null">
        fuel_litersPerHundredMiles = #{fuelLitersperhundredmiles,jdbcType=DOUBLE},
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
