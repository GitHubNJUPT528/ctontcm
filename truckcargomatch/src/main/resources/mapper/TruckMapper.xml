    insert into t_truck
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="truckType != null">
        truck_type,
      </if>
      <if test="count != null">
        count,
      </if>
      <if test="truckVolume != null">
        truck_volume,
      </if>
      <if test="truckLoad != null">
        truck_load,
      </if>
      <if test="fuelLitersperhundredmiles != null">
        fuel_litersPerHundredMiles,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="truckType != null">
        #{truckType,jdbcType=VARCHAR},
      </if>
      <if test="count != null">
        #{count},
      </if>
      <if test="truckVolume != null">
        #{truckVolume,jdbcType=DOUBLE},
      </if>
      <if test="truckLoad != null">
        #{truckLoad,jdbcType=DOUBLE},
      </if>
      <if test="fuelLitersperhundredmiles != null">
