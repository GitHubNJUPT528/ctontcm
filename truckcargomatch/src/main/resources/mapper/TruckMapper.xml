    <result column="count" jdbcType="INTEGER" property="count" />
    <result column="truck_volume" jdbcType="DOUBLE" property="truckVolume" />
    <result column="truck_load" jdbcType="DOUBLE" property="truckLoad" />
    <result column="fuel_litersPerHundredMiles" jdbcType="DOUBLE" property="fuelLitersperhundredmiles" />
  </resultMap>
  <sql id="Base_Column_List">
